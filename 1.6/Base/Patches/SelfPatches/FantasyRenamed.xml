<Patch>
  <!-- These patches are probably not going to be terribly fast...-->
  <Operation Class="BigAndSmall.PatchOp_SciFiEnabled">
    <match Class="BigAndSmall.PatchOp_ReplaceText">
      <xpath>Defs/*/*[self::label or self::description]</xpath>
      <success>Always</success>
      <oldTexts>
        <li>soul power</li>
      </oldTexts>
      <newTexts>
        <li>anomalous power</li>
      </newTexts>
    </match>
  </Operation>
